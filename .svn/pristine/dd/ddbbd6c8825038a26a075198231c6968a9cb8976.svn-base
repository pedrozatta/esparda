
br.com.esparda.nome=Esparda
br.com.esparda.info=${br.com.esparda.nome}: ${br.com.esparda.categoria.nome} ${br.com.esparda.categoria.versao}

br.com.esparda.categoria.nome=Microservice Categoria
br.com.esparda.categoria.versao=V1

br.com.esparda.categoria.datasource.url=jdbc:h2:file:./db/categoria
br.com.esparda.categoria.datasource.username=sa
br.com.esparda.categoria.datasource.password=sa
br.com.esparda.categoria.datasource.driverClassName=org.h2.Driver

server.port = 50113
#mover esse parametro para Controller
server.contextPath=/v1/categoria

spring.datasource.url=${br.com.esparda.categoria.datasource.url}
spring.datasource.username=${br.com.esparda.categoria.datasource.username}
spring.datasource.password=${br.com.esparda.categoria.datasource.password}
spring.datasource.driverClassName=${br.com.esparda.categoria.datasource.driverClassName}

spring.jpa.hibernate.ddl-auto = update
spring.jpa.hibernate.show-sql = true

oauth2.resource.userInfoUri=http://localhost:50000/auth-service/user

auth.server.url=http://localhost:50000/auth-service/oauth/check_token/
auth.server.clientId=soncrserv
auth.server.clientsecret=soncrserv

spring.activemq.broker-url=tcp://localhost:61616
spring.activemq.user=admin
spring.activemq.password=admin
spring.activemq.pooled=false

